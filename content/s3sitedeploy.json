{
    "object_specific": [
        {
            "path": ".*",
            "headers": { "Cache-Control": "max-age=3600" }
        },
        {
            "path": "^article/.*",
            "headers": { "Cache-Control": "max-age=3600" }
        },
        {
            "path": "^assets/.*",
            "headers": { "Cache-Control": "max-age=31104000" }
        }
    ],
    "gzip_mimetypes": [
        "text/html", "text/css", "text/plain", "text/yaml",
        "application/javascript"
    ]
}
